{
  "name": "mockapi-responder",
  "version": "1.0.0",
  "description": "A developer-friendly tool that spins up a mock API server from a single JSON or YAML file, no code required",
  "main": "dist/index.js",
  "bin": {
    "mockapi": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mock",
    "api",
    "server",
    "testing",
    "development",
    "prototyping",
    "cli",
    "faker",
    "json-server"
  ],
  "author": "Jayant Aggarwal <jadps2004@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jay-aggarwal/mockapi-responder.git"
  },
  "bugs": {
    "url": "https://github.com/jay-aggarwal/mockapi-responder/issues"
  },
  "homepage": "https://github.com/jay-aggarwal/mockapi-responder#readme"
}